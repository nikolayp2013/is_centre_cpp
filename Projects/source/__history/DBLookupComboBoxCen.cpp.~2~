//---------------------------------------------------------------------------

#include <vcl.h>

#pragma hdrstop

#include "DBLookupComboBoxCen.h"
#pragma package(smart_init)
//---------------------------------------------------------------------------
// ValidCtrCheck is used to assure that the components created do not have
// any pure virtual functions.
//

static inline void ValidCtrCheck(TDBLookupComboBoxCen *)
{
	new TDBLookupComboBoxCen(NULL);
}
//---------------------------------------------------------------------------
__fastcall TDBLookupComboBoxCen::TDBLookupComboBoxCen(TComponent* Owner)
	: TDBLookupComboBox(Owner)
{
}
//---------------------------------------------------------------------------
namespace Dblookupcomboboxcen
{
	void __fastcall PACKAGE Register()
	{
		TComponentClass classes[1] = {__classid(TDBLookupComboBoxCen)};
		RegisterComponents(L"Centre", classes, 0);
	}
}
//---------------------------------------------------------------------------
